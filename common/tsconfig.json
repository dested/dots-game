{
  "compilerOptions": {
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "allowUnreachableCode": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2019",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "sourceMap": true,
    "pretty": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ]
}
